<!doctype html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE"/>
    <meta charset="utf-8"/>
    <title>Gauge Test Results</title>
    <link rel="shortcut icon" type="image/x-icon" href="__ROOT_HREF__images/favicon.ico">
    <link rel="stylesheet" type="text/css" href="__ROOT_HREF__css/open-sans.css">
    <link rel="stylesheet" type="text/css" href="__ROOT_HREF__css/font-awesome.css">
    <link rel="stylesheet" type="text/css" href="__ROOT_HREF__css/normalize.css"/>
    <link rel="stylesheet" type="text/css" href="__ROOT_HREF__css/style.css"/>
</head>
<body>
<header class="top">
    <div class="header">
        <div class="container">
            <div class="logo">
                <a href="__ROOT_HREF__index.html"><img src="__ROOT_HREF__images/gaugeLogo.png" alt="Report logo"></a>
            </div>
            <h2 class="project">Project: gauge-tests</h2>
        </div>
    </div>
</header>
<main class="main-container">
    <div class="container">
        <div class="report-overview">
            <div class="report_chart">
                <div class="chart">
                    <svg id="pie-chart" data-results="__TOTAL_TESTS_FAILED__,__TOTAL_TESTS_PASSED__,__TOTAL_TESTS_SKIPPED__" data-total="__TOTAL_TESTS__">
                        <path class="status failed"/>
                        <path class="shadow failed" data-status="failed"><title>Failed: __TOTAL_TESTS_FAILED__/__TOTAL_TESTS__</title></path>
                        <path class="status passed"/>
                        <path class="shadow passed" data-status="passed"><title>Passed: __TOTAL_TESTS_PASSED__/__TOTAL_TESTS__</title></path>
                        <path class="status skipped"/>
                        <path class="shadow skipped" data-status="skipped"><title>Skipped: __TOTAL_TESTS_SKIPPED__/__TOTAL_TESTS__</title></path>
                    </svg>
                </div>
            </div>
            <div class="report_test-results">
                <div class="report_test-result specs">
                    <div class="total-specs" title="Filter all specs"><span class="txt">Total Specs</span><span
                            class="value">__TOTAL_SPECS__</span></div>
                    <div class="fail spec-filter" data-status="failed" title="Filter failed specs"><span
                            class="value">__TOTAL_SPECS_FAILED__</span></div>
                    <div class="pass spec-filter" data-status="passed" title="Filter passed specs"><span
                            class="value">__TOTAL_SPECS_PASSED__</span></div>
                    <div class="skip spec-filter" data-status="skipped" title="Filter skipped specs"><span
                            class="value">__TOTAL_SPECS_SKIPPED__</span></div>
                </div>
                <div class="report_test-result scenarios">
                    <div class="total-scenarios"><span class="txt">Total scenario</span><span class="value">__TOTAL_TESTS__</span></div>
                    <div class="fail scenario-stats" data-status="failed"><span class="value">__TOTAL_TESTS_FAILED__</span></div>
                    <div class="pass scenario-stats" data-status="passed"><span class="value">__TOTAL_TESTS_PASSED__</span></div>
                    <div class="skip scenario-stats" data-status="skipped"><span class="value">__TOTAL_TESTS_SKIPPED__</span></div>
                </div>
            </div>
            <div class="report_details">
                <ul>
                    <li>
                        <label>Environment</label>
                        <span>default</span>
                    </li>
                    <li>
                        <label>Tags</label>
                        <span>Not supported</span>
                    </li>
                    <li>
                        <label>Success Rate</label>
                        <span>__RATE__</span>
                    </li>
                    <li>
                        <label>Total Time</label>
                        <span>__TOTAL_TIME__</span>
                    </li>
                    <li>
                        <label>Generated On</label>
                        <span>__DATE__</span>
                    </li>
                </ul>
            </div>
        </div>
        <div class="specifications">
            <aside class="sidebar">
                <h3 class="title">Specifications</h3>
                <div class="searchbar">
                    <input id="searchSpecifications" placeholder="Type specification or tag name" type="text"/>
                    <i class="fa fa-search"></i>
                </div>
                <div class="specs-sorting">
                    <div class="sort sort-specs-name" data-sort-by="specs-name"><span class="sort-icons"><i
                            class="fa fa-caret-up"></i><i class="fa fa-caret-down"></i></span><span>Name</span></div>
                    <div class="sort sort-execution-time" data-sort-by="execution-time"><span class="sort-icons"><i
                            class="fa fa-caret-up"></i><i
                            class="fa fa-caret-down"></i></span><span>Execution time</span></div>
                </div>
                <div id="listOfSpecifications">
                    <ul id="scenarios" class="spec-list">
                        __SPEC_ROWS__
                    </ul>
                </div>
            </aside>
            <div id="specificationContainer" class="details">
                __SPEC_DETAILS__
            </div>
            <div class="congratulations details">
                <br><br><br>
                <p>__GREEN__</p>
            </div>
        </div>
    </div>
</main>
<footer class="footer">
    <div class="container">
        <p>Generated by Gauge HTML Report</p>
    </div>
</footer>
<script type="text/javascript">
      var loadingImage = "__ROOT_HREF__images/loading.gif";
      var closeButton = "__ROOT_HREF__images/close.gif";
</script>
<script src="__ROOT_HREF__js/lightbox.js"></script>
<script src="__ROOT_HREF__js/jquery-3.1.0.min.js" type="text/javascript"></script>
<script src="__ROOT_HREF__js/auto-complete.min.js" type="text/javascript"></script>
<script src="__ROOT_HREF__js/clipboard.min.js" type="text/javascript"></script>
<script src="__ROOT_HREF__js/search_index.js" type="text/javascript"></script>
<script src="__ROOT_HREF__js/main.js" type="text/javascript"></script>
</body>
</html>

